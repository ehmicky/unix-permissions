{
  "name": "unix-permissions",
  "version": "4.8.0",
  "type": "module",
  "exports": {
    "types": "./build/src/main.d.ts",
    "default": "./build/src/main.js",
    "import": "./build/src/main.js",
    "require": "./cjs/main.cjs"
  },
  "main": "./cjs/main.cjs",
  "module": "./build/src/main.js",
  "types": "./build/src/main.d.ts",
  "files": [
    "build/src/**/*.{js,json,d.ts}",
    "!build/src/**/*.test.js",
    "!build/src/{helpers,fixtures}",
    "cjs"
  ],
  "bin": {
    "unix-permissions": "build/src/bin/main.js"
  },
  "sideEffects": false,
  "scripts": {
    "prepare": "rollup -c",
    "test": "node test.cjs && gulp test"
  },
  "description": "Swiss Army knife for Unix permissions",
  "keywords": [
    "access-control",
    "acl",
    "chmod",
    "cli",
    "javascript",
    "library",
    "nodejs",
    "octal",
    "permissions",
    "posix",
    "setgid",
    "setuid",
    "shell",
    "stat",
    "sticky",
    "symbolic",
    "terminal",
    "typescript",
    "umask",
    "unix"
  ],
  "license": "Apache-2.0",
  "homepage": "https://www.github.com/ehmicky/unix-permissions",
  "repository": "ehmicky/unix-permissions",
  "bugs": {
    "url": "https://github.com/ehmicky/unix-permissions/issues"
  },
  "author": "ehmicky <ehmicky@gmail.com> (https://github.com/ehmicky)",
  "directories": {
    "doc": "docs",
    "lib": "src"
  },
  "dependencies": {
    "filter-obj": "^5.1.0",
    "handle-cli-error": "^3.1.1",
    "is-plain-obj": "^4.1.0",
    "read-pkg-up": "^9.1.0",
    "update-notifier": "^6.0.2",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@ehmicky/dev-tasks": "^2.0.60",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "execa": "^6.1.0",
    "get-bin-path": "^8.0.0",
    "rollup": "^3.10.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "test-each": "^5.7.1"
  },
  "engines": {
    "node": ">=14.18.0"
  }
}
